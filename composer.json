{
    "name": "joomla/di",
    "type": "joomla-package",
    "description": "Joomla DI Package",
    "keywords": ["joomla", "framework", "dependency injection", "di", "ioc", "container"],
    "homepage": "https://github.com/joomla-framework/di",
    "license": "GPL-2.0+",
    "require": {
        "php": "~7.0",
        "psr/container": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3",
        "squizlabs/php_codesniffer": "1.*",
        "symfony/yaml": "^3.3"
    },
    "provide": {
        "psr/container-implementation": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\DI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "3.0-dev"
        }
    },
    "suggest": {
        "symfony/yaml": "Allows to read Yaml config file"
    }
}
